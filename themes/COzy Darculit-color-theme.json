		"statusBar.noFolderForeground": "#bbbbbb",
		"tab.activeBackground": "#515658",
		"tab.activeBorder": "#4787C9",
		"tab.border": "#323232",
		"tab.hoverBackground": "#27292a",
		"tab.inactiveBackground": "#3c3f41",
		"tab.inactiveForeground": "#bbbbbb",
		"tab.unfocusedActiveForeground": "#bbbbbb",
		"tab.unfocusedHoverBackground": "#27292a",
		"tab.unfocusedInactiveForeground": "#bbbbbb",
		"terminal.background": "#2b2b2b",
		"terminal.foreground": "#bbbbbb",
		"titleBar.activeBackground": "#3c3f41",
		"titleBar.border": "#323232",
		"titleBar.inactiveBackground": "#313335",
		"notebook.focusedCellBorder": "#4787C9",
		"notebook.cellHoverBackground": "#313335",
		"notebook.focusedEditorBorder": "#505050",
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"typeParameter": "#679892",
		"variable.readonly": {
			"fontStyle": "bold"
		}
	},
	"tokenColors": [
		// GENERAL - COLOR GROUPS
		{
			"name": "types primitive/builtin",
			"scope": "support.type.primitive,support.type.builtin",
			"settings": {
				"foreground": "#7cbfee",
			}
		},
		{
			"name": "entity type name",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#a7dbff"
			}
		},
		{
			"name": "keywords: operators, constants; punctuation",
			"scope": [
				"keyword.control",
				"keyword.operator.expression",
				"constant",
				"storage.type",
				"storage.modifier",
				"punctuation.terminator",
				"punctuation.separator.comma"
			],
			"settings": {
				"foreground": "#cc8242"
			}
		},
		{
			"name": "Class",
			"scope": [
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#CFD4D7"
			}
		},
		{
			"name": "Numbers and RegEx",
			"scope": [
				"constant.numeric",
				"string.regexp"
			],
			"settings": {
				"foreground": "#6897BB"
			}
		},
		{
			"name": "function",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"entity.name.type.module"
			],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"name": "Documentation",
			"scope": [
				"comment.block.documentation",
				"storage.type.class.jsdoc"
			],
			"settings": {
				"foreground": "#629755"
			}
		},
				{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		// GENERAL - MODIFICATIONS
		{
			"name": "Italic",
			"scope": [
				// Keywords:
				"constant",
				"support.type.primitive",
				"support.type.builtin",
				// Property:
				"variable.other.object.property",
				"variable.other.property",
				"variable.other.constant.property",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "Underline",
			"scope": [
				// Class and Enum
				"entity.name.type.enum",
				"entity.name.type.class",
	
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Underline AND Italic",
			"scope": [
				// Documentation
				"storage.type.class.jsdoc"
			],
			"settings": {
				"fontStyle": "italic underline"
			}
		},
		{
			"name": "None (disable parent modifications)",
			"scope": [
				// Numbers
				"constant.numeric"
			],
			"settings": {
				"fontStyle": ""
			}
		},		
		// --> JSON -->
		{
			"name": "JSON string value",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		// --> HTML -->
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#FFC66D"// import from General function
			}
		},
		{
			"name": "attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#BABABA"// import from General function
			}
		},
		{
			"name": "Attribute value",
			"scope": [
				"string.quoted.double.html",
				"punctuation.separator.key-value.html"
			],
			"settings": {
				"foreground": "#A5C261"// import from General function
			}
		},
	]
}